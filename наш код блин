def razmn():
    t = int(input('\n–í–≤–µ–¥–∏—Ç–µ –≤—Ä–µ–º—è, –∑–∞ –ø—Ä–æ–º–µ–∂—É—Ç–æ–∫ –∫–æ—Ç–æ—Ä–æ–≥–æ —Ö–æ—Ç–∏—Ç–µ –≤—ã—á–∏—Å–ª–∏—Ç—å –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å —Ä–∞–∑–º–Ω–æ–∂–µ–Ω–∏—è: '))
    n = int(input('–í–≤–µ–¥–∏—Ç–µ –Ω–∞—á–∞–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ—Å–æ–±–µ–π –≤ –ø–æ–ø—É–ª—è—Ü–∏–∏: '))
    #x = int(input('–í–≤–µ–¥–∏—Ç–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ—Å–æ–±–µ–π, –∫–æ—Ç–æ—Ä–æ–µ —Ö–æ—Ç–∏—Ç–µ –ø–æ–ª—É—á–∏—Ç—å —á–µ—Ä–µ–∑ –∏–Ω—Ç–µ—Ä–≤–∞–ª –≤—Ä–µ–º–µ–Ω–∏ t: '))
    b = int(input('–í–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å –ø–æ—è–≤–ª–µ–Ω–∏—è –Ω–æ–≤–æ–π –æ—Å–æ–±–∏ —É 1 –ø–æ—Ç–æ–º–∫–∞ –∑–∞ –∏–Ω—Ç–µ—Ä–≤–∞–ª t: '))  
    x=3 # x —Ö–∑ —á–æ —Ç–∞–∫–æ–µ –∞—Ö—Ö–∞—Ö–∞—Ö
    print("\n–§–æ—Ä–º—É–ª—ã:\n\n1) P = (1-e^(Œ≤t)(1-x^(-1)))^(-a)")
    p1 = pow((1 - pow(2.7, b*t) * (1 - pow(x, (-1)))), (-a))# –≤–∞—â–µ —Ç—É—Ç –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å —Ñ–æ—Ä–º—É–ª–∞ —Å –º–∞—Ç—Ä–∏—Ü–µ–π 
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p1)
    print("\n2) P = —á—Ç–æ-—Ç–æ —Ç–∞–º ")  # –Ω–µ –∑–Ω–∞—é –∫–∞–∫–∏–µ –µ—â–µ —Ñ–æ—Ä–º—É–ª—ã –µ—Å—Ç—å=> –Ω–µ –¥—É–º–∞—é —á—Ç–æ —Ç—É—Ç –Ω—É–∂–Ω–æ –µ—â–µ —á—Ç–æ —Ç–æ, –Ω–æ –º–æ–∂–Ω–æ –±—É–¥–µ—Ç –ø–æ—Ç–æ–º –¥–æ–±–∞–≤–∏—Ç—å –º–∞—Ç.–æ–∂–∏–¥–∞–Ω–∏–µ –∏ –¥–∏—Å–ø–µ—Ä—Å—Å–∏—é
    p2 = 1
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p2)

def razm_gib():
    t = int(input('\n–í–≤–µ–¥–∏—Ç–µ –≤—Ä–µ–º—è, –∑–∞ –ø—Ä–æ–º–µ–∂—É—Ç–æ–∫ –∫–æ—Ç–æ—Ä–æ–≥–æ —Ö–æ—Ç–∏—Ç–µ –≤—ã—á–∏—Å–ª–∏—Ç—å –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å —Ä–∞–∑–º–Ω–æ–∂–µ–Ω–∏—è: '))
    n = int(input('–í–≤–µ–¥–∏—Ç–µ –Ω–∞—á–∞–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ—Å–æ–±–µ–π: '))
    b = int(input('–í–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å –ø–æ—è–≤–ª–µ–Ω–∏—è –Ω–æ–≤–æ–π –æ—Å–æ–±–∏ —É 1 –ø–æ—Ç–æ–º–∫–∞ –∑–∞ –∏–Ω—Ç–µ—Ä–≤–∞–ª t: ')) 
    m = int (input('–í–≤–µ–¥–∏—Ç–µ –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å –≥–∏–±–µ–ª–∏ 1 –æ—Å–æ–±–∏ –∑–∞ –∏–Ω—Ç–µ—Ä–≤–∞–ª t: '))
    print("\n–§–æ—Ä–º—É–ª—ã:\n\n1) P=(1-eÀÜ(Œ≤t)(1-xÀÜ(-1)))ÀÜ(-a)")
    p1 = pow((1 - pow(2.7, 2) * (1 - pow(x, (-1)))), (-2))#—Ç—É—Ç –≤—Ä–æ–¥–µ —Ç–æ–∂–µ –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –¥—Ä—É–≥–∞—è—è—è—è—è —É—Ö 
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p1)
    print("\n2) P=—á—Ç–æ-—Ç–æ —Ç–∞–º ")
    p2 = 1
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p2)

def migr():
    t = int(input('–í–≤–µ–¥–∏—Ç–µ –≤—Ä–µ–º—è: '))
    n = int(input('–í–≤–µ–¥–∏—Ç–µ –Ω–∞—á–∞–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ: '))
    b = 12   # –Ω—É–∂–Ω–æ –ø–æ–¥—Å—Ç–∞–≤–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏—è –Ω–µ –∑–Ω–∞—é –∫–∞–∫–∏–µ
    u = 33
    v = 42
    X = 10
    T = pow(2.7, ((b-u)*t))
    print("\n–§–æ—Ä–º—É–ª—ã:\n\n1) P = (((Œ≤-Œº)/(Œ≤*T-Œº))^(ŒΩ/Œ≤))*(1-((Œ≤*(T-1))/(Œ≤*T-Œº))*X)^(-ŒΩ/Œ≤)")
    p1 = pow(((b-u)/(b*T-u)), (v/b))*pow((1-((b*(T-1))/(b*T-u))*X), (-v/b))
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p1)
    print("\n2) P = —á—Ç–æ-—Ç–æ —Ç–∞–º ")
    p2 = 1
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p2)

def popul():
    t = int(input('–í–≤–µ–¥–∏—Ç–µ –≤—Ä–µ–º—è: '))
    n = int(input('–í–≤–µ–¥–∏—Ç–µ –Ω–∞—á–∞–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ: '))
    print("\n–§–æ—Ä–º—É–ª—ã:\n\n1) P=(1-eÀÜ(Œ≤t)(1-xÀÜ(-1)))ÀÜ(-a)")
    p1 = pow((1 - pow(2.7, 2) * (1 - pow(x, (-1)))), (-2))# –±–ª–∏–Ω–∞ –Ω–µ –º–æ–≥—É –ø–æ–Ω—è—Ç—å –≥–¥–µ —Ç—ã –≤–∑—è–ª–∞ —ç—Ç—É —Ñ–æ—Ä–º—É–ª—É
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p1)
    print("\n2) P=—á—Ç–æ-—Ç–æ —Ç–∞–º ")
    p2 = 1
    print("–†–µ–∑—É–ª—å—Ç–∞—Ç: ", p2)


print('\n–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ –ø—Ä–æ–≥—Ä–∞–º–º—É –ö—É–∑–Ω–µ—Ü–æ–≤–æ–π –ï–∫–∞—Ç–µ—Ä–∏–Ω—ã –∏ –ë–∞–±—É—Ö–∞–¥–∏—è –ú–∞—Ä–≥–∞—Ä–∏—Ç—ã üôÇ\n')
print('–í—ã–±–µ—Ä–∏—Ç–µ –≥–ª–∞–≤—É:\n')
print('1) –ü—Ä–æ—Å—Ç–æ–π –ø—Ä–æ—Ü–µ—Å—Å —Ä–∞–∑–º–Ω–æ–∂–µ–Ω–∏—è\n')
print('2) –ü—Ä–æ—Ü–µ—Å—Å —Ä–∞–∑–º–Ω–æ–∂–µ–Ω–∏—è –∏ –≥–∏–±–µ–ª–∏\n')
print('3) –í–ª–∏—è–Ω–∏–µ –º–∏–≥—Ä–∞—Ü–∏–∏\n')
print('4) –ü–æ–ø—É–ª—è—Ü–∏–∏, —Å–æ—Å—Ç–æ—è—â–∏–µ –∏–∑ –æ—Å–æ–±–µ–π 2 –ø–æ–ª–æ–≤\n')

a = input("–í—ã–±–µ—Ä–∏—Ç–µ –ø—É–Ω–∫—Ç –º–µ–Ω—é: ")
while a < "1" or a > "4":
    a = input("–í—ã–±–µ—Ä–∏—Ç–µ –ø—É–Ω–∫—Ç –º–µ–Ω—é: ")
if a == "1":
    razmn()
elif a == "2":
    razm_gib()
elif a == "3":
    migr()
elif a == "4":
    popul()
